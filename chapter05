def print_j(t):
 t.back(25)
 t.left(90)
 t.forward(25)
 t.left(180)
 t.pendown()
 arc(t, 25, 180)
 t.forward(75)
 t.left(90)
 t.forward(50)
 t.back(50)
 t.penup()
 t.left(90)
 t.forward(100)
 t.left(90)
 t.forward(50)
 
def print_k(t):
 t.pendown()
 t.left(90)
 t.forward(100)
 t.back(50)
 cos_line_with_an_angle(t, 50, -45)
 t.penup()
 t.right(45)
 t.forward(100)
 t.left(90)
 t.pendown()
 cos_line_with_an_angle(t, -50, -45)
 t.penup()
 t.left(45)
 cos_line_with_an_angle(t, -50, -45)
 t.left(45)
 t.forward(50)

