def print_g(t):
    t.right(-90)
    t.forward(25)
    t.pendown()
    t.forward(50)
    arc(t, 25, -180)
    t.right(180)
    arc(t, 25, 180)
    t.forward(50)
    arc(t, 25, 180)
    t.right(180)
    t.back(25)
    t.left(90)
    t.right(90)
    t.forward(50)
    t.left(90)
    t.penup()
    t.forward(50)

def print_h(t):
    t.pendown()
    t.left(90)
    t.forward(100)
    t.back(50)
    t.right(90)
    t.forward(50)
    t.left(90)
    t.forward(50)
    t.back(100)
    t.right(90)
    t.penup()
    t.forward(50)
 
